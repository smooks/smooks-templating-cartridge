<?xml version="1.0"?>
<smooks-resource-list xmlns="https://www.smooks.org/xsd/smooks-1.2.xsd" xmlns:jb="https://www.smooks.org/xsd/smooks/javabean-1.6.xsd" default-target-profile="shipping-response">

	<!--
		Use a XSL template to perform the transformation.
		We're transforming the "history" element...
	-->
	<resource-config selector="history">
        <resource>/org/smooks/cartridges/templating/soapshipping/xslt/HistoryTrans.xsl</resource>
    </resource-config>

    <!--
        Configure the History bean creation and population...
    -->
    <jb:bean beanId="history" class="org.smooks.cartridges.templating.soapshipping.History" createOnElement="history">
        <jb:value property="trackingNumbers" data="history" />
    </jb:bean>

</smooks-resource-list>
